# AIVEN_PG_US_EAST=postgres://user:pass@host:6543/db?sslmode=require
# AIVEN_PG_EU_WEST=postgres://user:pass@host:6543/db?sslmode=require
# AIVEN_PG_ASIA_PACIFIC=postgres://user:pass@host:6543/db?sslmode=require